  
{
    "name": "Telehealth",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Development Product.",
    "permissions": [
        "sidePanel",
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "action": {
        "default_title": "Click to open panel"
    },
    "side_panel": {
        "default_path": "sidepanel.html"
    }
}
